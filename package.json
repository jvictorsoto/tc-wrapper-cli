{
  "name": "tccli",
  "version": "1.0.1",
  "author": "J. Victor Soto",
  "license": "MIT",
  "description": "NodeJS cli for tc-wrapper (a library for traffic control on linux)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvictorsoto/tc-wrapper-cli.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "babel ./src -d ./dist --ignore test.js",
    "pretest": "npm run build",
    "test": "mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register ./src/**/*.test.js --recursive",
    "test:single": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "debug": "^2.6.8",
    "tc-wrapper": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "mocha": "^3.1.2",
    "sinon": "^2.3.2"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "tccli": "./dist/index.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "keywords": [
    "tc",
    "traffic control",
    "traffic shapping",
    "netem"
  ],
  "bugs": {
    "url": "https://github.com/jvictorsoto/tc-wrapper-cli/issues"
  },
  "homepage": "https://github.com/jvictorsoto/tc-wrapper-cli#readme"
}
